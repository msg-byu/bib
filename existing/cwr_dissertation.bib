title = {{Fast Parallel Algorithms for Short – Range Molecular Dynamics}},
volume = {117},
year = {1995}
}
@article{Tokar1997,
abstract = {A cluster approach to lattice statistical models is proposed based on the functional formulation used earlier in the gamma-expansion method (GEM). In some respects the approach is superior to the GEM by allowing, for example, for the treatment of models with cluster interactions as well as giving more accurate results. It is argued that the proposed method may turn out to be more efficient than the CVM because (i) it accounts for intersite pair interactions at all distances irrespective of the size of the cluster used in the approximation; (ii) in preliminary calculations it showed much quicker convergence and (iii) with the increase of the cluster size the number of equations grows much more slowly than in the CVM. Besides, the equations are derived via a simple standard routine. This makes feasible their derivation with the use of software for analytical computation, which may be crucial in dealing with large clusters.},
