journal = {Physical Review B},
number = {16},
pages = {11169--11186},
pmid = {9984901},
title = {{Efficient iterative schemes for ab initio total-energy calculations using a plane-wave basis set}},
url = {http://link.aps.org/doi/10.1103/PhysRevB.54.11169},
volume = {54},
year = {1996}
}
