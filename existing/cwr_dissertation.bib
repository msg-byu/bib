doi = {10.1103/PhysRevB.44.512},
file = {:Users/andrewhuynguyen/Dropbox/Mendeley/Lu et al. - 1991 - First-principles statistical mechanics of structural stability of intermetallic compounds.pdf:pdf},
issn = {0163-1829},
journal = {Physical Review B},
number = {2},
pages = {512--544},
