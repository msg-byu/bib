eprint = {1209.3140v2},
file = {:Users/andrewhuynguyen/Dropbox/Mendeley/Bart{\'{o}}k, Kondor, Cs{\'{a}}nyi - 2013 - On representing chemical environments.pdf:pdf},
issn = {10980121},
journal = {Physical Review B - Condensed Matter and Materials Physics},
number = {18},
