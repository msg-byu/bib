isbn = {0927-0256},
issn = {09270256},
journal = {Computational Materials Science},
keywords = {alloys,atomic interaction,cluster-expansion,energetics,miscibility gap,monte-carlo,scattering,short-range order,solid-solutions,thermodynamics},
number = {1-2},
pages = {107--121},
title = {{Ni-Au: A testing ground for theories of phase stability}},
url = {{\textless}Go to ISI{\textgreater}://A1997XE91000016},
volume = {8},
year = {1997}
}
@article{VandeWalle2013,
abstract = {Traditional first-principles calculations excel at providing formation$\backslash$nenergies at absolute zero, but obtaining thermodynamic information at$\backslash$nnonzero temperatures requires suitable sampling of all the excited$\backslash$nstates visited in thermodynamic equilibrium, which would be$\backslash$ncomputationally prohibitive via brute-force quantum mechanical$\backslash$ncalculations alone. In the context of solid-state alloys, this issue can$\backslash$nbe addressed via the coarse-graining concept and the cluster expansion$\backslash$nformalism. This process generates simple, effective Hamiltonians that$\backslash$naccurately reproduce quantum mechanical calculation results and that can$\backslash$nbe used to efficiently sample configurational, vibrational, and$\backslash$nelectronic excitations and enable the prediction of thermodynamic$\backslash$nproperties at nonzero temperatures. Vibrational and electronic degrees$\backslash$nof freedom are formally eliminated from the problem by writing the$\backslash$nsystem's partition function in a nested form in which the inner sums can$\backslash$nbe readily evaluated to yield an effective Hamiltonian. The remaining$\backslash$noutermost sum corresponds to atomic configurations and can be handled$\backslash$nvia Monte Carlo sampling driven by the resulting effective Hamiltonian,$\backslash$nthereby delivering thermodynamic properties at nonzero temperatures.$\backslash$nThis article describes these techniques and their implementation in the$\backslash$nalloy theoretic automated toolkit, an open-source software package. The$\backslash$nmethods are illustrated by applications to various alloy systems.},
