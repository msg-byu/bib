@article{ptmo_jlcm_1972,
	Abstract = {
An investigation was conducted to determine the effects of alloy additions of Hf, Ta, W, Re, Os, Ir, and Pt on the hardness of Mo. Special emphasis was placed on alloy softening in these binary Mo alloys. A modified micro-hardness test unit permitted hardness determinations at homologous temperatures ranging from 0.02 to 0.15, where alloy softening normally occurs in b.c.c. alloys.
Results showed that alloy softening was produced by those elements having an excess of s + d electrons compared to Mo while those elements having an equal number or fewer s+d electrons than Mo failed to produce alloy softening. The magnitude of the softening and the amount of solute element at the hardness minimum diminished rapidly with increasing test temperature. At solute concentrations where alloy softening was observed, the temperature sensitivity of hardness was lowered. For solute elements having an excess of s+d electrons or fewer s+d electrons than Mo, alloy softening and alloy hardening can be correlated with the difference in number ofs + d electrons of the solute element and Mo.},
	Author = {Joseph R. Stephens and Walter R. Witzke},
	Doi = {DOI: 10.1016/0022-5088(72)90201-9},
	Issn = {0022-5088},
	Journal = {Journal of the Less Common Metals},
	Number = {4},
	Pages = {371 - 388},
	Title = {Alloy hardening and softening in binary molybdenum alloys as related to electron concentration},
	Url = {http://www.sciencedirect.com/science/article/B759D-48F69S0-J/2/450dbb5a32b188e7eb38633b077f322b},
